@page
@model YouthAtHeart.Pages.TestFAQModel
@{
}
<h1>Send any of your questions here!</h1>
<form method="post">
    <div class="form-group">
        <label>Email: </label>
        <input type="email" asp-for="MyQuestions.Email" class="form-control col-sm-3" id="email" />
    </div>
    <div class="form-group">
        <label>Question: </label>
        <textarea asp-for="MyQuestions.Question" id="Qn" rows="8" cols="80" class="form-control">
            </textarea>
    </div>
    <input type="submit" class="btn btn-primary" />
</form>