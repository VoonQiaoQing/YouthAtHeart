@page
@using YouthAtHeart.Models
@model YouthAtHeart.Pages.FAQSendModel
@{
    ViewData["Title"] = "Send";
}
@*<!DOCTYPE HTML>
    <html>
    <head>
        <title>FAQSend</title>
    </head>*@
<!--<body>-->
<h1>
    Have your questions sent here!
</h1>
<form method="post">
    <div class="form-group">
        <label>Email: </label>
        <input type="email" asp-for="MyQuestions.Email" class="form-control col-sm-3" id="email" />
    </div>
    <div class="form-group">
        <label>Question: </label>
        <textarea asp-for="MyQuestions.Question" id="Qn" rows="8" cols="80" class="form-control">
            </textarea>
    </div>
    <input type="submit" class="btn btn-primary" />
</form>
@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
